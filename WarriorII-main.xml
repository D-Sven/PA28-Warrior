		<sound>
			<path>Aircraft/PA28-Warrior/Pa28-sound.xml</path>
		</sound>

		<view n="0">
			<name>Pilot View</name>
			<internal archive="y">true</internal>
			<config>
				<x-offset-m archive="y">-0.24</x-offset-m>
				<y-offset-m archive="y">0.5</y-offset-m>
				<z-offset-m archive="y">2.6</z-offset-m>
				<pitch-offset-deg>-15</pitch-offset-deg>
				<default-field-of-view-deg type="double">82</default-field-of-view-deg>
			</config>
		</view>
		<view n="1">
			<internal archive="y">true</internal>
			<name>Helicopter View</name>
			<config>
				<target-z-offset-m archive="y" type="double">3.0</target-z-offset-m>
				<x-offset-m archive="y">0</x-offset-m>
				<y-offset-m archive="y">0</y-offset-m>
				<z-offset-m archive="y">0</z-offset-m>
				<default-field-of-view-deg type="double">66</default-field-of-view-deg>
				<heading-offset-deg>-35</heading-offset-deg>
			</config>
		</view>
		<view n="2">
			<internal archive="y">true</internal>
			<name>Chase View</name>
			<config>
				<target-z-offset-m archive="y" type="double">3.0</target-z-offset-m>
				<x-offset-m archive="y">0</x-offset-m>
				<y-offset-m archive="y">0</y-offset-m>
				<z-offset-m archive="y">0</z-offset-m>
				<default-field-of-view-deg type="double">66</default-field-of-view-deg>
				<heading-offset-deg>-35</heading-offset-deg>
			</config>
		</view>
		<view n="7">
			<config>
				<internal archive="y">true</internal>
				<name>Model View</name>
				<target-z-offset-m archive="y" type="double">3.0</target-z-offset-m>
				<x-offset-m archive="y">0</x-offset-m>
				<y-offset-m archive="y">0</y-offset-m>
				<z-offset-m archive="y">0</z-offset-m>
				<default-field-of-view-deg type="double">66</default-field-of-view-deg>
				<heading-offset-deg>35</heading-offset-deg>
			</config>
		</view>
		<view n="5">
			<internal archive="y">true</internal>
			<name>Chase without Yaw</name>
			<config>
				<x-offset-m archive="y">25</x-offset-m>
				<y-offset-m archive="y">2</y-offset-m>
				<z-offset-m archive="y">-10</z-offset-m>
				<default-field-of-view-deg type="double">47</default-field-of-view-deg>
				<heading-offset-deg>110</heading-offset-deg>
			</config>
		</view>
		<view n="100">
			<name>Copilot View</name>
			<type>lookfrom</type>
			<internal archive="y">true</internal>
			<config>
				<from-model type="bool">true</from-model>
				<x-offset-m archive="y">0.24</x-offset-m>
				<y-offset-m archive="y">0.5</y-offset-m>
				<z-offset-m archive="y">2.6</z-offset-m>
				<pitch-offset-deg>-15</pitch-offset-deg>
				<default-field-of-view-deg type="double">82</default-field-of-view-deg>
			</config>
		</view>
		
