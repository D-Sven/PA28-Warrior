<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <keyboard>

 <key n="66">
  <name>B</name>
  <desc>Toggle parking brake on or off</desc>
  <binding>
   <command>nasal</command>
   <script>controls.applyParkingBrake(1)</script>
  </binding>
  <mod-up>
   <binding>
    <command>nasal</command>
    <script>controls.applyParkingBrake(0)</script>
   </binding>
</mod-up>	
	<binding>
	<condition>
		<not>
		<property>controls/gear/brake-parking</property>
		</not>
	</condition>	
			<command>show-message</command>
			<id>strap</id>
			<label>Fasten Your Seat Belt</label>
			<delay>3.75</delay>
	</binding>
 </key>


   <key n="102">
   <name>f</name>
   <desc>full throttle</desc>
   <binding>
                    <command>property-cycle</command>
                    <property>controls/engines/engine/throttle</property>
<value>1</value>
<value>0</value>
                </binding>
   </key>



  <key n="107">
  <name>k</name>
  <desc>Show Map</desc>
  <binding>
				<command>dialog-show</command>
				<dialog-name>map</dialog-name>
			</binding>
  </key>
  
  <key n="114">
  <name>r</name>
  <desc>Show RM</desc>
  <binding>
				<command>dialog-show</command>
				<dialog-name>route-manager</dialog-name>
			</binding>
  </key>


  <key n="119">
  <name>w</name>
  <desc>Show AC Config</desc>
  <binding>
			<command>nasal</command>
				<script>											acconfig.main_dlg.open();
				</script>
  </binding>
  </key>

  <key n="12">
  <name>Ctrl-L</name>
  <desc>toggle panel lights</desc>
                <binding>
                    <command>property-cycle</command>
                    <property>controls/switches/panel-lights-factor</property>
<value>1</value>
<value>0</value>

                </binding>
            </key>



  <key n="64">
  <name>@</name>
  <desc>toggle alternator</desc>
                <binding>
                    <command>property-cycle</command>
                    <property>controls/electrical/alternator</property>
<value>1</value>
<value>0</value>

                </binding>
            </key>

  <key n="33">
  <name>!</name>
  <desc>toggle battery</desc>
                <binding>
                    <command>property-cycle</command>
                    <property>controls/electrical/battery</property>
<value>1</value>
<value>0</value>

                </binding>
            </key>



  <key n="106">
  <name>j</name>
  <desc>toggle tanks</desc>
                <binding>
                    <command>property-cycle</command>
                    <property>systems/fuel/selected-tank</property>
<value>1</value>
<value>2</value>

                </binding>
            </key>
 

<key n="35">
  <name>#</name>
  <desc>toggle fuelpump</desc>
                <binding>
                    <command>property-cycle</command>
                    <property>controls/switches/fuel-pump</property>
<value>1</value>
<value>0</value>

                </binding>
            </key>

<key n="42">
  <name>*</name>
  <desc>toggle pitot heat</desc>
                <binding>
                    <command>property-cycle</command>
                    <property>controls/switches/pitot-heat</property>
<value>1</value>
<value>0</value>

                </binding>
            </key>

<key n="100">
  <name>d</name>
  <desc>toggle door</desc>
                <binding>
                    <command>property-cycle</command>
                    <property>sim/model/door-positions/rightDoor/position-norm</property>
<value>0</value>
<value>1</value>
                </binding>
<binding>
	<condition>
		<property>sim/model/door-positions/rightDoor/position-norm</property>
	</condition>
			<command>show-message</command>
			<id>openclose</id>
			<label>Door is Open </label>
			<property>sim/model/door-positions/rightDoor/position-norm</property>
	<value>1</value>
			<delay>1.75</delay>
		</binding>

<binding>
	<condition>
		<not>
		<property>sim/model/door-positions/rightDoor/position-norm</property>
		</not>
	</condition>
			<command>show-message</command>
			<id>openclose</id>
			<label>Door is Closed </label>
			<property>sim/model/door-positions/rightDoor/position-norm</property>
	<value>0</value>
			<delay>1.75</delay>
		</binding>


            </key>

<key n="117">
  <name>u</name>
  <desc>toggle carb heat</desc>
                <binding>
                    <command>property-cycle</command>
                    <property>controls/anti-ice/engine/carb-heat</property>
<value>false</value>
<value>true</value>

                </binding>
            </key>








</keyboard>

</PropertyList>













