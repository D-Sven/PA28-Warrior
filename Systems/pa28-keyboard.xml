<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <keyboard>

  <key n="107">
  <name>k</name>
  <desc>Show Map</desc>
  <binding>
				<command>dialog-show</command>
				<dialog-name>map</dialog-name>
			</binding>
  </key>
  
  <key n="114">
  <name>r</name>
  <desc>Show RM</desc>
  <binding>
				<command>dialog-show</command>
				<dialog-name>route-manager</dialog-name>
			</binding>
  </key>


  <key n="119">
  <name>w</name>
  <desc>Show AC Config</desc>
  <binding>
			<command>nasal</command>
				<script>											acconfig.main_dlg.open();
				</script>
  </binding>
  </key>

  <key n="12">
  <name>Ctrl-L</name>
  <desc>toggle panel lights</desc>
                <binding>
                    <command>property-cycle</command>
                    <property>controls/switches/panel-lights-factor</property>
<value>1</value>
<value>0</value>

                </binding>
            </key>


</keyboard>

</PropertyList>













