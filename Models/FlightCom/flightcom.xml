<?xml version="1.0"?>

<PropertyList>

 <path>FlightCom.ac</path>

 <animation>
  <type>material</type>
  <object-name>Face</object-name>
  <emission>
   <red>1.0</red>
   <green>0.2</green>
   <blue>0.0</blue>
   <factor-prop>/sim/model//material/instruments/factor</factor-prop>
  </emission>
 </animation>
 
 <animation>
    <type>pick</type>
    <object-name>Face</object-name>
    <action>
    <button>0</button>
    <repeatable>false</repeatable>
          <binding>
             <command>property-adjust</command>
             <property>controls/engines/engine/throttle</property>
             <step>.25</step>
          </binding>
     </action>
  </animation>

</PropertyList>



