<?xml version="1.0"?>

<PropertyList>

 <path>hobbs.ac</path>

 <animation>
  <type>material</type>
  <object-name>HobbsFace</object-name>
  <emission>
   <red>1.0</red>
   <green>0.2</green>
   <blue>0.0</blue>
   <factor-prop>/sim/model//material/instruments/factor</factor-prop>
  </emission>
  </animation>
   
  <animation>
  <type>pick</type>
  <object-name>HobbsFace</object-name>
  <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>hobbs</tooltip-id>
                <label>Hours Flown</label>
            </binding>
        </hovered>
  </animation>

</PropertyList>





