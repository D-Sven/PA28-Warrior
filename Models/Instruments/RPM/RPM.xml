<?xml version="1.0"?>

<PropertyList>
	
	<path>RPM.ac</path>
	
	<animation>
		<type>material</type>
		<object-name>Face</object-name>
		<object-name>PointerTip</object-name>
		<emission>
			<red>0.6</red>
			<green>0.6</green>
			<blue>0.6</blue>
			<factor-prop>controls/switches/panel-lights</factor-prop>
		</emission>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>PointerCentre</object-name>
		<object-name>PointerTip</object-name>
		<property>engines/engine[0]/rpm</property>
		<interpolation>
			<entry><ind>   0</ind><dep> 18</dep></entry>
			<entry><ind> 300</ind><dep> 20</dep></entry>
			<entry><ind> 500</ind><dep> 32</dep></entry>
			<entry><ind>1000</ind><dep> 70</dep></entry>
			<entry><ind>1500</ind><dep>108</dep></entry>
			<entry><ind>2000</ind><dep>146</dep></entry>
			<entry><ind>2500</ind><dep>184</dep></entry>
			<entry><ind>3000</ind><dep>230</dep></entry>
			<entry><ind>3500</ind><dep>268</dep></entry>
		</interpolation>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>0</z-m>
		</center>
	</animation>

</PropertyList>





